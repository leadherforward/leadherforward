*
  box-sizing: border-box

.clearfix
  *zoom: 1
  &:before, &:after
    content: ""
    display: table
  &:after
    clear: both

.none, .hide, .js-hide
  display: none

h1
  margin-bottom 30px

body
  padding-top: 50px
  padding-bottom: 50px
  background-color: #F5F5F5

body div.content
  background-color: white
  padding: 10px
  border-radius(5px)

ul
  margin: 0 0 9px 17px